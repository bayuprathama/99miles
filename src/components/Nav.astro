---
import { navItem } from '../data'
---

<nav class="bg-gray-900 text-gray-50 lg:grid lg:grid-cols-12">
  <ul
    class="flex gap-8 justify-center items-center py-6 col-span-12 lg:col-span-5"
  >
    {
      navItem.map((item) => (
        <li>
          <a href={item.url} class="uppercase tracking-[0.2rem] text-sm">
            {item.name}
          </a>
        </li>
      ))
    }
  </ul>
  <div class="lg:col-span-7 bg-gray-50"></div>
</nav>
